.row
  .col-md-4
  .col-md-4
    .login-form
      %center
        %h2 Log in
      = simple_form_for resource, url: session_path(resource_name) do |f|
        = f.input :email
        = f.input :password
        = f.button :submit, "Log in",  class: 'btn'
        
      = render "devise/shared/links"
    
  .col-md-4
